@model List<ResultBookingDto>
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="active-member">
                    <div class="table-responsive">
                        <table class="table table-xs mb-0">
                            <h1>Son 5 rezervasyon</h1>
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Ad Soyad</th>
                                    <th>Mail</th>
                                    <th>Status</th>
                                    <th>Giriş tarihi</th>
                                    <th>Çıkış Tarihi</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                   
                                    <tr>
                                        <td><img src="~/Images/user.png" class=" rounded-circle mr-3" alt=""></td>
                                        <td>@item.Name</td>
                                        <td>@item.Mail</td>
                                        <td>
                                            <span>@item.SpecialRequest</span>
                                        </td>
                                        <td>
                                            <div>
                                                <div class="progress" style="height: 6px">
                                                    <div class="progress-bar bg-success" style="width: 20%">@item.Description</div>
                                                </div>
                                            </div>
                                        </td>
                                        <td><i class="fa fa-circle-o text-success  mr-2"></i> @item.Checkin.ToShortDateString()</td>
                                        <td>
                                            <span class="m-0 pl-3">@item.ChechkOut.ToShortDateString()</span>
                                        </td>
                                    </tr>

                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
